{
    "version": 2,
    "builds": [
      {
        "src": "backend/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/auth/(.*)",
        "dest": "/backend/routes/auth.js"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/public/$1"
      }
    ]
  }